{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "noEmitOnError": true,
    "target": "es2020",
    "lib": [
      "esnext"
    ],
    "plugins": [],
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "."
  ]
}
